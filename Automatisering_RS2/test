import source.filbehandling.make_objects as mo
import zipfile
c = r"C:\Users\Eirik\OneDrive\Documents\10. Prosjekt og masteroppgave\modellering_svakhetssone\parameterstudie\Mine modeller\RS2\tverrsnitt_sirkul√¶r\arbeidsfiler\S_bm80_ss1_k1_od100_m2\rs2\S_bm80_ss1_k1_od100_m2_v10_y0_x0.fez "
print(c)
c = mo.alternate_slash([c])[0]
zipdata = zipfile.ZipFile(c)
zipinfos = zipdata.infolist()

# iterate through each file
for zipinfo in zipinfos:
    # This will do the renaming
    zipinfo.filename = 'S_bm80_ss1_k1_od100_m2_v10_y0_x0'
    zipdata.extract(zipinfo)
